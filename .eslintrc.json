{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "no-console": 0,
        "max-len": 0,
        "camelcase": 0   
    }
}
